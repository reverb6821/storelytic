{
  "name": "storelytic",
  "version": "1.0.0",
  "description": "storelytic app",
  "main": "index.js",
  "scripts": {
    "app:serve:dev": "cd api/ && npm run start",
    "client:serve:dev": "cd client/ && npm run dev",
    "api:install": "cd api/ && npm install",
    "client:install": "cd client/ && npm install",
    "api:migration":"cd api && npm run db:migrate",
    "api:seeds":"cd api && npm run db:seed:all",
    "db:migration:seeds":"npm run api:migration && npm run api:seeds",
    "app:install":"concurrently \"npm run api:install\" \"npm run client:install\"",
    "app:dev":"concurrently \"npm run app:serve:dev\" \"npm run client:serve:dev\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reverb6821/storelytic.git"
  },
  "author": "Giuseppe Gigliotti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reverb6821/storelytic/issues"
  },
  "homepage": "https://github.com/reverb6821/storelytic#readme",
  "dependencies": {
    "concurrently": "^7.4.0",
    "react-icons": "^4.4.0"
  }
}
